{
  "name": "a_certify_link",
  "version": "1.0.0",
  "description": "Digital Certificate Management Backend for Insurance",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node -r tsconfig-paths/register dist/server.js",
    "dev": "ts-node -r tsconfig-paths/register src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "db:create": "sequelize-cli db:create",
    "db:drop": "sequelize-cli db:drop",
    "db:migrate": "sequelize-cli db:migrate",
    "db:seed": "sequelize-cli db:seed:all",
    "db:seed:undo": "sequelize-cli db:seed:undo:all",
    "db:seed:status": "sequelize-cli db:seed:status",
    "db:setup": "npm run db:migrate && npm run db:seed",
    "db:reset": "sequelize-cli db:drop && sequelize-cli db:create && npm run db:setup",
    "seed:run": "node seeders/run-seeders.js"
  },
  "dependencies": {
    "@types/pg": "^8.15.4",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "i18next": "^23.7.6",
    "i18next-fs-backend": "^2.3.1",
    "i18next-http-middleware": "^3.5.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mssql": "^11.0.1",
    "opossum": "^8.0.1",
    "oracledb": "^6.8.0",
    "pg": "^8.16.2",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.35.0",
    "speakeasy": "^2.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tedious": "^18.6.1",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.3",
    "@types/opossum": "^8.1.8",
    "@types/oracledb": "^6.6.1",
    "@types/qrcode": "^1.5.5",
    "@types/speakeasy": "^2.0.10",
    "@types/supertest": "^2.0.16",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "sequelize-cli": "^6.6.2",
    "sequelize-cli-typescript": "^3.2.0-c",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "private": "true"
}
